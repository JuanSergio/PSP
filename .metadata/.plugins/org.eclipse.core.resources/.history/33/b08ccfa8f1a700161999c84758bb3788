package Ejercicios;

public class HiloEjercicio9{
	public static void main(String args[]){
		RunnableClass rc = new RunnableClass();
		int inicio= 0;
		Thread hilo1 = new Thread(rc);
		Thread hilo2 = new Thread(rc);
		Thread hilo3 = new Thread(rc);
		Thread hilo4 = new Thread(rc);
		Thread hilo5 = new Thread(rc);
		hilo1.setName("Hilo1");
		hilo2.setName("Hilo2");
		hilo3.setName("Hilo3");
		hilo4.setName("Hilo4");
		hilo5.setName("Hilo5");
		hilo1.start();
		hilo2.start();
		hilo3.start();
		hilo4.start();
		hilo5.start();
		
		
	}
}
